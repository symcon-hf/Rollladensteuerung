{
  "elements": [
    {
      "type": "ExpansionPanel",
      "caption": "Instanzinformationen",
      "items": [
        {
          "type": "Image",
          "image": "data:image/png;base64,"
        },
        {
          "type": "Label",
          "caption": "Ein Gemeinschaftsprojekt von Normen Thiel und Ulrich Bittner\n"
        },
        {
          "type": "Label",
          "caption": "Mit dieser Instanz können HomeMatic (HM-LC-Bl1-FM) und Homematic IP (HmIP-BROLL, HmIP-FROLL) Rollladenaktoren gesteuert werden.\nWeitere Informationen entnehmen Sie bitte der Dokumentation.\n"
        },
        {
          "type": "Label",
          "caption": "Symcon Version:\t5.2>"
        },
        {
          "type": "Label",
          "caption": "Name:\t\t\tRollladensteuerung"
        },
        {
          "type": "Label",
          "caption": "Version:\t\t\t1.00-6"
        },
        {
          "type": "Label",
          "caption": "Datum:\t\t\t06. Januar 2020"
        },
        {
          "type": "Label",
          "caption": "Uhrzeit:\t\t\t18:00"
        },
        {
          "type": "Label",
          "caption": "Entwickler:\t\tUlrich Bittner"
        },
        {
          "type": "Label",
          "caption": "Präfix:\t\t\tRS"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Anzeigeoptionen",
      "items": [
        {
          "type": "Label",
          "caption": "Bitte wählen Sie aus, welche Funktionen und Informationen angezeigt werden sollen:"
        },
        {
          "type": "CheckBox",
          "name": "EnableAutomaticMode",
          "caption": "Automatik"
        },
        {
          "type": "CheckBox",
          "name": "EnableWeeklySchedule",
          "caption": "Wochenplan"
        },
        {
          "type": "CheckBox",
          "name": "EnableDoorWindowState",
          "caption": "Tür- / Fensterstatus"
        },
        {
          "type": "CheckBox",
          "name": "EnableBlindSlider",
          "caption": "Rolladensteuerung"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Rollladenaktor",
      "items": [
        {
          "type": "Label",
          "caption": "Bitte wählen Sie den Gerätetypen aus:"
        },
        {
          "type": "Select",
          "name": "DeviceType",
          "caption": "Gerätetyp",
          "options": [
            {
              "caption": "Bitte auswählen",
              "value": 0
            },
            {
              "caption": "HM-LC-Bl1-FM",
              "value": 1
            },
            {
              "caption": "HmIP-BROLL",
              "value": 2
            },
            {
              "caption": "HmIP-FROLL",
              "value": 3
            }
          ]
        },
        {
          "type": "Label",
          "caption": "\nBitte wählen Sie die Variable für die Rollladenposition (HM: Kanal 1 LEVEL, HmIP: Kanal 3 LEVEL) aus:"
        },
        {
          "type": "SelectVariable",
          "name": "BlindLevel",
          "caption": "Rollladenposition"
        },
        {
          "type": "Label",
          "caption": "\nBitte wählen Sie die Variable für den Aktivitätszustand (HM: Kanal 1 WORKING, HmIP: Kanal 3 PROCESS) aus:"
        },
        {
          "type": "SelectVariable",
          "name": "BlindLevelProcess",
          "caption": "Aktivitätszustand"
        },
        {
          "type": "Label",
          "caption": "\nBitte wählen Sie die Variable für die Rollladensteuerung (HM: Kanal 1 LEVEL, HmIP: Kanal 4 LEVEL) aus:"
        },
        {
          "type": "SelectVariable",
          "name": "BlindActuator",
          "caption": "Steuerung"
        },
        {
          "type": "Label",
          "caption": "\nBitte wählen Sie die tatsächliche Logik des Rollladenaktors aus:"
        },
        {
          "type": "Select",
          "name": "BlindActuatorProperty",
          "caption": "Logik",
          "options": [
            {
              "caption": "0% = geschlossen / 100% = geöffnet",
              "value": 0
            },
            {
              "caption": "0% = geöffnet / 100% = geschlossen",
              "value": 1
            }
          ]
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Rollladenpositionen",
      "items": [
        {
          "type": "Label",
          "caption": "Bitte geben Sie die Rollladenposition in % für den geschlossenen Zustand an:"
        },
        {
          "type": "NumberSpinner",
          "name": "BlindPositionClosed",
          "caption": "Position",
          "suffix": "%"
        },
        {
          "type": "Label",
          "caption": "\nBitte geben Sie die Rollladenposition in % für den geöffneten Zustand an:"
        },
        {
          "type": "NumberSpinner",
          "name": "BlindPositionOpened",
          "caption": "Position",
          "suffix": "%"
        },
        {
          "type": "Label",
          "caption": "\nBitte wählen Sie aus, ob eine Positionsprüfung verwendet werden soll:"
        },
        {
          "type": "CheckBox",
          "name": "UseCheckBlindPosition",
          "caption": "Positionsprüfung verwenden"
        },
        {
          "type": "Label",
          "caption": "\nBitte geben Sie den Differenzwert in % an:"
        },
        {
          "type": "NumberSpinner",
          "name": "BlindPositionDifference",
          "caption": "Differenzwert",
          "suffix": "%"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Astrofunktion",
      "items": [
        {
          "type": "Label",
          "caption": "Bitte wählen Sie die Variable für den Sonnenuntergang (Rollladen runterfahren) aus:"
        },
        {
          "type": "SelectVariable",
          "name": "Sunset",
          "caption": "Sonnenuntergang"
        },
        {
          "type": "Label",
          "caption": "Bitte wählen Sie die Variable für den Sonnenaufgang (Rollladen hochfahren) aus:"
        },
        {
          "type": "SelectVariable",
          "name": "Sunrise",
          "caption": "Sonnenaufgang"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Wochenplan",
      "items": [
        {
          "type": "Label",
          "caption": "Bitte wählen Sie den Wochenplan aus:"
        },
        {
          "type": "SelectEvent",
          "name": "WeeklySchedule",
          "caption": "Wochenplan"
        },
        {
          "type": "Label",
          "caption": "\nBitte wählen Sie aus, ob bei der Aktivierung der Automatik die Rollladenposition direkt angepasst werden soll:"
        },
        {
          "type": "CheckBox",
          "name": "AdjustBlindLevel",
          "caption": "Rollladenposition anpassen"
        },
        {
          "type": "Label",
          "caption": "\nBitte geben Sie die Ausführungsverzögerung in Sekunden (0 = deaktiviert) für den Wochenplan an:"
        },
        {
          "type": "NumberSpinner",
          "name": "ExecutionDelay",
          "caption": "Ausführungsverzögerung",
          "suffix": "s"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Tür- / Fensterüberwachung",
      "items": [
        {
          "type": "Label",
          "caption": "Bitte wählen Sie die Variablen der Tür- und Fenstersensoren (HM: Kanal 1 STATE, HmIP: Kanal 1 STATE) aus:"
        },
        {
          "type": "List",
          "name": "DoorWindowSensors",
          "caption": "Tür- und Fenstersensoren",
          "rowCount": 3,
          "add": true,
          "delete": true,
          "sort": {
            "column": "ID",
            "direction": "ascending"
          },
          "columns": [
            {
              "name": "UseSensor",
              "caption": "Aktiviert",
              "width": "100px",
              "add": true,
              "edit": {
                "type": "CheckBox"
              }
            },
            {
              "name": "ID",
              "caption": "Variable",
              "width": "450px",
              "add": 0,
              "edit": {
                "type": "SelectVariable"
              }
            }
          ]
        },
        {
          "type": "Label",
          "caption": "\nBitte wählen Sie aus, ob ein Aussperrschutz verwendet werden soll:"
        },
        {
          "type": "CheckBox",
          "name": "UseLockoutProtection",
          "caption": "Aussperrschutz verwenden"
        },
        {
          "type": "Label",
          "caption": "\nBitte geben Sie die Rollladenposition in % (100% = immer) für den Aussperrschutz an:"
        },
        {
          "type": "NumberSpinner",
          "name": "LockoutPosition",
          "caption": "Rollladenposition",
          "suffix": "%"
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "ExpansionPanel",
      "caption": "Registrierte Nachrichten",
      "items": [
        {
          "type": "Button",
          "caption": "Registrierte Nachrichten anzeigen",
          "onClick": "RS_ShowRegisteredMessages($id);"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Wochenplan",
      "items": [
        {
          "type": "Button",
          "caption": "Aktuelle Aktion anzeigen",
          "onClick": "RS_ShowActualAction($id);"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Rollladen",
      "items": [
        {
          "type": "Button",
          "caption": "Rollladen runterfahren",
          "onClick": "RS_ToggleBlind($id, false, false);"
        },
        {
          "type": "Button",
          "caption": "Rollladen hochfahren",
          "onClick": "RS_ToggleBlind($id, true, false);"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Testbereich",
      "items": [
        {
          "type": "TestCenter"
        }
      ]
    }
  ],
  "status": [
    {
      "code": 101,
      "icon": "active",
      "caption": "Rollladensteuerung wird erstellt"
    },
    {
      "code": 102,
      "icon": "active",
      "caption": "Rollladensteuerung ist aktiv"
    },
    {
      "code": 103,
      "icon": "active",
      "caption": "Rollladensteuerung wird gelöscht"
    },
    {
      "code": 104,
      "icon": "inactive",
      "caption": "Rollladensteuerung ist inaktiv"
    },
    {
      "code": 200,
      "icon": "inactive",
      "caption": "Es ist Fehler aufgetreten, weitere Informationen unter Meldungen oder im Log"
    }
  ]
}

