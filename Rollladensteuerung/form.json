    {
      "type": "ExpansionPanel",
      "caption": "Wochenplan",
      "items": [
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "Button",
              "caption": "Aktuelle Aktion anzeigen",
              "onClick": "RS_ShowActualAction($id);"
            }
          ]
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Rollladen",
      "items": [
        {
          "type": "Button",
          "caption": "Rollladen runterfahren",
          "onClick": "RS_ToggleBlind($id, false, false);"
        },
        {
          "type": "Button",
          "caption": "Rollladen hochfahren",
          "onClick": "RS_ToggleBlind($id, true, false);"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Testbereich",
      "items": [
        {
          "type": "TestCenter"
        }
      ]
    }
